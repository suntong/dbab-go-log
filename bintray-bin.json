{
    "package": {
        "name": "dbab-svr",
        "repo": "bin",
        "subject": "suntong",
        "desc": "Pixel Server dbab-svr in Go",
        "website_url": "https://github.com/suntong/dbab-go",
        "issue_tracker_url": "https://github.com/suntong/dbab-go/issues",
        "vcs_url": "https://github.com/suntong/dbab-go.git",
        "licenses": ["MIT"],
        "public_download_numbers": false
    },

    "version": {
